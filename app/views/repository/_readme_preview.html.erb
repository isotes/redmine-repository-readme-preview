<div class="repository-readme-preview">
	<h3><%= filename.html_safe %></h3>
	<div class="wiki">
		<% if file_too_large %>
			<p><%= l(:file_too_large) %></p>
		<% else %>
				<% if html.html_safe.respond_to?(:force_encoding) %>
					<%= html.html_safe.force_encoding('UTF-8') %>
				<% else %>
					<%= html.html_safe %>
				<% end %>
		<% end %>
	</div>
</div>
